# API Configuration
# Copy this file to .env.local and fill in your API keys

# Feature flags for enabling real APIs (set to 'true' to enable)
NEXT_PUBLIC_USE_REAL_SPOTIFY_API=false
NEXT_PUBLIC_USE_REAL_YOUTUBE_API=false

# Spotify API Credentials
# Get these from https://developer.spotify.com/dashboard/
NEXT_PUBLIC_SPOTIFY_CLIENT_ID=your_spotify_client_id_here
NEXT_PUBLIC_SPOTIFY_CLIENT_SECRET=your_spotify_client_secret_here

# YouTube API Key
# Get this from https://console.cloud.google.com/
NEXT_PUBLIC_YOUTUBE_API_KEY=your_youtube_api_key_here

# OpenAI API Settings (for song analysis)
OPENAI_API_KEY=your_openai_api_key_here
OPENAI_MODEL=gpt-4o  # or gpt-3.5-turbo 